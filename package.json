{
  "name": "jsapi-check",
  "displayName": "jsapi-check",
  "description": "A VSCode plugin to check Chrome compatibility",
  "version": "0.0.1",
  "publisher": "zhichao.gui",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/guizhichao/jsapi-check.git"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:vue"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.checkCompatibility",
        "title": "Check Chrome Compatibility"
      }
    ],
    "languages": [
      {
        "id": "javascript",
        "extensions": [".js", ".jsx", ".JS"],
        "aliases": ["JavaScript", "javascript", "JS", "js", "jsx"]
      },
      {
        "id": "typescript",
        "extensions": [".ts", ".tsx", ".TS"],
        "aliases": ["Typescript", "typescript", "ts", "tsx", "TS"]
      },
      {
        "id": "vue",
        "extensions": [".vue", ".VUE"],
        "aliases": ["vue", "vue", "Vue"]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "@mdn/browser-compat-data": "5.6.7",
    "@vue/compiler-sfc": "3.5.12",
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "devDependencies": {
    
  }
}

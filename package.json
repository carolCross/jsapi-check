{
  "name": "jsapi-check",
  "displayName": "jsapi-check",
  "description": "A VSCode plugin to check Chrome compatibility（一款vscode插件用于检测浏览器jsapi的兼容性）",
  "version": "0.0.8",
  "publisher": "cross",
  "engines": {
    "vscode": "^1.94.0"
  },
  "pricing": "Free",
  "categories": [
    "Other"
  ],
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "jsapiCheckView",
          "name": "JS API Check"
        }
      ]
    },
    "languages": [
      {
        "id": "javascript",
        "extensions": [
          ".js",
          ".jsx",
          ".JS"
        ],
        "aliases": [
          "JavaScript",
          "javascript",
          "JS",
          "js",
          "jsx"
        ]
      },
      {
        "id": "typescript",
        "extensions": [
          ".ts",
          ".tsx",
          ".TS"
        ],
        "aliases": [
          "Typescript",
          "typescript",
          "ts",
          "tsx",
          "TS"
        ]
      }
    ]
  },
  "dependencies": {
    "@babel/parser": "^7.25.9",
    "@babel/traverse": "^7.25.9",
    "@babel/types": "^7.25.9",
    "@mdn/browser-compat-data": "5.6.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guizhichao/jsapi-check.git"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.6",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  }
}

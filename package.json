{
  "name": "jsapi-check",
  "displayName": "jsapi-check",
  "description": "A VSCode plugin to check browser API compatibility (Chrome/Safari/Safari iOS)（一款vscode插件用于检测浏览器jsapi的兼容性）support MDN document",
  "version": "2.1.1",
  "publisher": "cross",
  "engines": {
    "vscode": "^1.72.0"
  },
  "pricing": "Free",
  "categories": [
    "Other"
  ],
  "icon": "assets/logo.png",
  "scripts": {
    "vscode:prepublish": "yarn run build --production",
    "compile": "tsc -p ./",
    "watch": "node esbuild.js --watch",
    "build": "node esbuild.js",
    "pretest": "yarn run build && yarn run lint && yarn run compile",
    "lint": "eslint src",
    "test": "vscode-test",
    "version:patch": "standard-version --release-as patch",
    "version:minor": "standard-version --release-as minor",
    "version:major": "standard-version --release-as major",
    "version:custom": "standard-version --release-as",
    "version:auto": "node scripts/version.js",
    "publish:patch": "yarn version:patch && vsce publish --no-yarn",
    "publish:minor": "yarn version:minor && vsce publish --no-yarn",
    "publish:major": "yarn version:major && vsce publish --no-yarn",
    "publish:auto": "yarn version:auto && vsce publish --no-yarn",
    "package": "vsce package"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:vue"
  ],
  "browser": "./dist/extension.js",
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "jsapiCheckView",
          "name": "JS API Check"
        }
      ]
    },
    "languages": [
      {
        "id": "javascript",
        "extensions": [
          ".js",
          ".jsx",
          ".JS",
          ".mjs",
          ".MJS",
          ".cjs",
          ".CJS"
        ],
        "aliases": [
          "JavaScript",
          "javascript",
          "JS",
          "js",
          "jsx"
        ]
      },
      {
        "id": "typescript",
        "extensions": [
          ".ts",
          ".tsx",
          ".TS"
        ],
        "aliases": [
          "Typescript",
          "typescript",
          "ts",
          "tsx",
          "TS"
        ]
      },
      {
        "id": "vue",
        "extensions": [
          ".vue",
          ".VUE"
        ],
        "aliases": [
          "vue",
          "vue",
          "Vue"
        ]
      }
    ],
    "commands": [
      {
        "command": "jsapi-check.checkCompatibility",
        "title": "checkCompatibility",
        "category": "JS API Check"
      }
    ]
  },
  "dependencies": {
    "@babel/parser": "^7.25.9",
    "@babel/traverse": "^7.25.9",
    "@babel/types": "^7.25.9",
    "@mdn/browser-compat-data": "5.6.7"
  },
  "bugs": {
    "url": "https://github.com/carolCross/jsapi-check/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carolCross/jsapi-check.git"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.6",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.9",
    "eslint": "^9.11.1",
    "standard-version": "^9.5.0",
    "typescript": "^5.6.2"
  },
  "packageManager": "yarn@4.9.3+sha512.8295ee814f6b253e16f516416481b481a215ed03ef1ae38524d108084872560a9ed75aeb23b91ab64222062ac4149a594150ae538c2a9536fdbcefd4e49b11cc"
}
